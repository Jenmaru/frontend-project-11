{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
    	"no-console": 0,
    	"import/extensions": 0,
    	"no-undef": 0,
    	"no-underscore-dangle": 0,
    	"import/no-extraneous-dependencies": 0,
    	"object-shorthand": 0,
    	"no-param-reassign": 0,
    	"no-unused-expressions": 0
    }
}
